		"node_modules/*",
		"build/*",
		"dist/*"
	],
	"dependencies": {
		"@enact/core": "^4.8.0",
		"@enact/i18n": "^4.8.0",
		"@enact/moonstone": "^4.5.6",
		"@enact/sandstone": "^2.8.0",
		"@enact/spotlight": "^4.8.0",
		"@enact/ui": "^4.8.0",
		"ilib": "^14.19.0",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"web-vitals": "^3.5.1"
	},
	"devDependencies": {
		"eslint-config-enact-proxy": "^1.0.7"
	}
