{
	"name": "hello-webcam-app-new",
	"version": "1.0.0",
	"description": "LG WebOS Webcam App for viewing webcam feeds on LG OLED65C8 TV",
	"author": "MorgeWeb",
	"main": "src/index.js",
	"scripts": {
		"serve": "enact serve",
		"pack": "enact pack",
		"pack-p": "enact pack -p",
		"watch": "enact pack --watch",
		"clean": "enact clean",
		"lint": "enact lint .",
		"license": "enact license",
